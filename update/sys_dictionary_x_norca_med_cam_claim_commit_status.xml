        <audit>false</audit>
        <calculation><![CDATA[(function calculatedFieldValue(current) {

	if ("oasissn".equalsIgnoreCase(current.getValue("sys_updated_by"))) {
		return "success";
	}
	if (current.getValue("claim_id")) {
		if (current.getValue("last_committed_update")==current.getValue("sys_mod_count")) {
			return "success";
		}
		
	} else if (current.getValue("claim_log_id")) {
		if (current.getValue("cel_last_committed_update")==current.getValue("sys_mod_count") && (!current.coverage_verification_status || current.coverage_verification_status=="P" || current.coverage_verification_status=="Pending")) {
			return "success";
		}
		
	} else if (!current.u_insured || !current.claimant || !current.u_loss_date || !current.report_date ) {
		return "na";
		
	} else if (!current.last_update_includes_sor_fields) {
		return "success";
	}
	
	return gs.minutesAgoEnd( gs.getProperty( "x_norca_med_cam.commit_window", 15))>current.getValue("sys_updated_on") ? "failure" : "pending";

})(current);]]></calculation>
